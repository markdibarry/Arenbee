config_version=5

[application]

config/name="Arenbee"
run/main_scene="res://Framework/Game/GameRoot.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Godot_User_Data"
config/icon="res://icon.png"
config/features=PackedStringArray("4.0", "C#", "Vulkan Clustered")

[debug]

shapes/collision/shape_color=Color(1, 1, 1, 0.419608)

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/window_width_override=1440
window/size/window_height_override=810
window/stretch/mode="viewport"

[gui]

theme/custom="res://Assets/GUI/Themes/BaseTheme.tres"
theme/default_font_antialiased=false
fonts/dynamic_fonts/use_oversampling=false

[importer_defaults]

font_data_dynamic={
"antialiased": false
}
texture={
"compress/hdr_compression": 0,
"detect_3d/compress_to": 0,
"process/hdr_as_srgb": true
}

