config_version=5

[application]

config/name="Arenbee"
run/main_scene="res://Arenbee/Game/GameRoot.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Godot_User_Data"
config/features=PackedStringArray("4.0", "C#", "Forward Plus")
