[autoload]

LocatorAutoload="*res://Arenbee/Game/LocatorAutoload.cs"

[debug]

shapes/collision/shape_color=Color(1, 1, 1, 0.419608)

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/window_width_override=1440
window/size/window_height_override=810
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Arenbee"

[editor]

scene/scene_naming=1

[gui]

theme/custom="res://Arenbee/GUI/Themes/BaseTheme.tres"
theme/default_font_antialiasing=0
theme/default_font_subpixel_positioning=0
theme/lcd_subpixel_layout=0
fonts/dynamic_fonts/use_oversampling=false
theme/default_font_antialiased=false

[importer_defaults]

font_data_dynamic={
"antialiasing": 0
}
texture={
"compress/hdr_compression": 0,
"detect_3d/compress_to": 0,
"process/hdr_as_srgb": true
